                                            word_find = reverseString(word_find);                                    
                                        }
                                        
                                        ds_map_replace(map_lista, "cor_" + string(word_find), list_font_revela_color);
                                        
                                    }
                                }
                                else
                                {
                                    if(gx &gt; fcx)
                                    {
                                        for(k = 0; k &lt; word_find_length; k++)
